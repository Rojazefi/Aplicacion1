@model IEnumerable<MvcApplication1.Models.tb_Alumno>

@{
    ViewBag.Title = "ReportsAlumnos";
}

<h2>ReportsAlumnos</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<div><a href="@Url.Action("ExportReport")"> Get Reporte PDF </a></div>

<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.cod_alumno)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nom_alumno)
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.cod_escuela)*@
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.cod_alumno)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nom_alumno)
        </td>
        <td>
            @*@Html.DisplayFor(modelItem => item.cod_escuela)*@
        </td>
    </tr>
}

</table>
